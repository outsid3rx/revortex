{
  "name": "revortex",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "tsdown --no-clean --minify",
    "build:dev": "tsdown --sourcemap --no-clean --watch ./src",
    "run": "node ./dist/index.js",
    "run:dev": "node --watch ./dist/index.js",
    "lint": "eslint \"./src/**/*.ts\" --max-warnings=0",
    "lint:fix": "eslint \"./src/**/*.ts\" --max-warnings=0 --fix"
  },
  "keywords": [
    "nest",
    "fetch",
    "ky"
  ],
  "author": "Max Zernyaev <winterzz@internet.ru>",
  "license": "MIT",
  "description": "Generate REST API for frontend projects from Nest controllers",
  "dependencies": {
    "commander": "^12.1.0",
    "readdirp": "^4.0.2",
    "ts-morph": "^26.0.0",
    "typescript": "^5.6.2",
    "winston": "^3.15.0",
    "zod": "^3.23.8",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.7.4",
    "eslint": "^8.57.1",
    "eslint-kit": "^11.19.0",
    "ky": "^1.7.2",
    "lefthook": "^1.7.18",
    "prettier": "^3.3.3",
    "tsdown": "^0.14.1"
  },
  "bugs": {
    "url": "https://github.com/outsid3rx/revortex/issues"
  },
  "homepage": "https://github.com/outsid3rx/revortex",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/outsid3rx/revortex.git"
  }
}
